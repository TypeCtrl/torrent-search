{
  "name": "@ctrl/torrent-search",
  "version": "0.0.0-placeholder",
  "description": "Search torrent sites using definition files from jackett and cardigann",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/torrent-search",
  "keywords": [
    "tracker",
    "torrent",
    "search",
    "typescript"
  ],
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@ctrl/cloudflare": "^1.1.0",
    "@ctrl/golang-template": "^1.1.3",
    "@ctrl/tracker-definitions": "^1.1.24",
    "bytes": "^3.1.0",
    "cheerio": "^1.0.0-rc.2",
    "cloudscraper": "^2.0.0",
    "date-fns": "^2.0.0-alpha.27",
    "date-fns-tz": "^1.0.7",
    "filenamify": "^2.1.0",
    "got": "^9.6.0",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "tough-cookie": "^3.0.1",
    "ua-string": "^2.1.0"
  },
  "devDependencies": {
    "@types/query-string": "6.2.0",
    "@types/bytes": "3.0.0",
    "@types/cheerio": "0.22.10",
    "@types/filenamify": "2.0.1",
    "@types/got": "9.4.1",
    "@types/jest": "24.0.8",
    "@types/lodash": "4.14.121",
    "@types/nock": "9.3.1",
    "@types/node": "11.9.5",
    "@types/tough-cookie": "2.3.5",
    "@types/ua-string": "2.0.0",
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "@typescript-eslint/parser": "1.4.2",
    "eslint": "5.14.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.8.0",
    "eslint-plugin-import": "2.16.0",
    "jest": "24.1.0",
    "jest-junit": "6.3.0",
    "nock": "10.0.6",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "timekeeper": "2.1.2",
    "ts-jest": "24.0.0",
    "ts-node": "8.0.2",
    "typescript": "3.3.3333"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
