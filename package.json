{
  "name": "@ctrl/torrent-search",
  "version": "0.0.0-placeholder",
  "description": "Search torrent sites using definition files from jackett and cardigann",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/torrent-search",
  "keywords": [
    "tracker",
    "torrent",
    "search",
    "typescript"
  ],
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@ctrl/cloudflare": "^1.4.1",
    "@ctrl/golang-template": "^1.1.3",
    "@ctrl/tracker-definitions": "^1.1.88",
    "bytes": "^3.1.0",
    "cheerio": "^1.0.0-rc.3",
    "date-fns": "^2.0.0-alpha.27",
    "date-fns-tz": "^1.0.7",
    "filenamify": "^4.0.0",
    "got": "^9.6.0",
    "lodash": "^4.17.21",
    "query-string": "^6.5.0",
    "tough-cookie": "^3.0.1",
    "ua-string": "^3.0.0"
  },
  "devDependencies": {
    "@types/query-string": "6.3.0",
    "@types/bytes": "3.1.0",
    "@types/cheerio": "0.22.18",
    "@types/got": "9.6.11",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.157",
    "@types/nock": "10.0.3",
    "@types/node": "12.12.47",
    "@types/tough-cookie": "2.3.7",
    "@types/ua-string": "2.0.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.11.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "24.9.0",
    "jest-junit": "6.4.0",
    "nock": "10.0.6",
    "prettier": "1.19.1",
    "rimraf": "2.7.1",
    "semantic-release": "15.14.0",
    "timekeeper": "2.2.0",
    "ts-jest": "24.3.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
